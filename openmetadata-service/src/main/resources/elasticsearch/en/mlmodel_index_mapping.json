          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "word_delimiter_filter",
            "om_stemmer"
          ]
        },
        "om_ngram": {
          "type": "custom",
          "tokenizer": "edge_ngram_tokenizer",
          "filter": ["lowercase"]
        }
      },
      "filter": {
        "om_stemmer": {
          "type": "stemmer",
          "name": "kstem"
        },
        "word_delimiter_filter": {
          "type": "word_delimiter",
          "preserve_original": true
        }
      }
    }
  },
