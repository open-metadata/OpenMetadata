{
  "name": "RdfIndexApp",
  "displayName": "RDF Knowledge Graph Indexing",
  "boundType": "Global",
  "configuration": {
    "globalAppConfig": {
      "config": {
        "entities": [
          "all"
        ],
        "recreateIndex": false,
        "batchSize": "100"
      },
      "schedule": {
        "scheduleTimeline": "Custom",
        "cronExpression": "0 0 * * *"
      }
    }
  },
  "supportsInterrupt": true
}