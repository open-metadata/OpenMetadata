{
  "name": "DataInsightsApplication",
  "displayName": "Data Insights",
  "boundType": "Global",
  "configuration": {
    "globalAppConfig": {
      "config": {
        "batchSize": 100,
        "recreateDataAssetsIndex": false,
        "backfillConfiguration": {
          "enabled": false
        },
        "moduleConfiguration": {
          "dataAssets": {
            "enabled": true,
            "entities": [
              "all"
            ],
            "retention": 7
          },
          "appAnalytics": {
            "enabled": true
          },
          "dataQuality": {
            "enabled": true
          },
          "costAnalysis": {
            "enabled": true
          }
        }
      },
      "schedule": {
        "scheduleTimeline": "Custom",
        "cronExpression": "0 3 * * *"
      }
    }
  }
}