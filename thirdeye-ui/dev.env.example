# Copy this file to .env.local for development

# OpenMetadata Backend Configuration
OPENMETADATA_BASE_URL=http://localhost:8585
DEV_SERVER_TARGET=http://localhost:8585/

# Development Mode options
NODE_ENV=development
NEXT_PUBLIC_OPENMETADATA_BASE_URL=http://localhost:8585

# Authentication (for production setup)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

